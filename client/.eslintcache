mponents/StandingsPage';\n// import ClubsPage from './components/ClubsPage';\n// import TabPage from './components/TabPage/TabPage';\nimport 'bootstrap/dist/css/bootstrap.min.css';\n\n\n\nfunction App() {\n  return (\n    <Router>\n      <div>\n        <NavbarObject/>\n        <Switch>\n          <Route exact path=\"/\" component={HomePage}/>\n          {/* <Route exact path=\"/Fixtures\" component={FixturesPage}/>           \n          <Route exact path=\"/Results\" component={ResultsPage}/>  */}\n          <Route exact path=\"/Standings\" component={StandingsPage}/> \n          {/* <Route exact path=\"/Clubs\" component={ClubsPage}/>\n          <Redirect exact from=\"/Clubs/:clubId/\" to=\"/Clubs/:clubId/Fixtures\" />\n          <Route exact path=\"/Clubs/:clubId/:page?\" component={TabPage} /> */}\n        </Switch>\n      </div>\n    </Router>\n    \n  );\n}\n\nexport default App;\n","C:\\Users\\andye\\OneDrive\\Desktop\\PL-site\\premierleague + express\\client\\src\\components\\StandingsPage.js",[],"C:\\Users\\andye\\OneDrive\\Desktop\\PL-site\\premierleague + express\\client\\src\\components\\Navbar\\NavbarObject.js",["49"],"import React, { Component } from 'react'\r\nimport { Navbar, Nav, } from 'react-bootstrap';\r\nimport $ from 'jquery';\r\nimport SearchIcon from '@material-ui/icons/Search';\r\n\r\nexport default class NavbarObject extends Component {\r\n    constructor(props){\r\n    super(props)\r\n    this.state = {\r\n        teams: [],\r\n        suggestions: [],\r\n        text: '',\r\n        }\r\n    }\r\n\r\n    async componentDidMount(){\r\n      fetch('/teams')\r\n        .then(res => res.json())\r\n        .then(teams => this.setState({teams}))\r\n    }\r\n\r\n    handleText = (e) => {\r\n        const value = e.target.value;\r\n        let suggestions = [];\r\n        if(value.length > 0){\r\n            const regex = new RegExp(`^${value}`, 'i');\r\n            const names = []\r\n            for(let i = 0; i < this.state.teams.length; i++){\r\n                names.push(this.state.teams[i].name)\r\n            }\r\n            suggestions = names.sort().filter(v => regex.test(v))\r\n        }\r\n        this.setState(() => ({ suggestions, text: value }))\r\n    }\r\n\r\n    suggestionSelected(value){\r\n        this.setState(() => ({\r\n            text: value,\r\n            suggestions: []\r\n        }))\r\n    }\r\n\r\n\r\n    renderSuggestions(){\r\n        const { suggestions } = this.state;\r\n        if(suggestions.length === 0){\r\n            return null;\r\n        }\r\n        const names = []\r\n        for(let i = 0; i < this.state.teams.length; i++){\r\n            names.push(this.state.teams[i].name)\r\n        }\r\n        return(\r\n            <ul className=\"search-ul\">\r\n                {suggestions.map((item) => <li className=\"li-selections\" onClick={() => this.suggestionSelected(item)}>{item}</li>)}\r\n            </ul>\r\n        )\r\n    }\r\n\r\n    handleKeyDown = (e) => {\r\n        if(e.key === 'Enter'){\r\n            let getClubId;\r\n            for(let i = 0; i < this.state.teams.length; i++){\r\n                if(this.state.teams[i].name === e.target.value){\r\n                    getClubId = this.state.teams[i].id;\r\n                }\r\n            }\r\n            window.location.href=`/Clubs/${getClubId}`\r\n        }\r\n    }\r\n\r\n    render() {\r\n        // console.log(this.state.teams)\r\n        const { text } = this.state;\r\n       \r\n        return (\r\n            <div>\r\n                <Navbar className=\"color-nav\" variant=\"dark\" expand=\"lg\" style={{marginBottom: \"50px\"}}>\r\n                    <Nav className=\"mr-auto\" style={{fontSize: \"2em\", fontWeight: \"500\", }}>\r\n                        <Nav.Link  href=\"/\">Home</Nav.Link>\r\n                        <Nav.Link  href=\"/Fixtures\">Fixtures</Nav.Link>\r\n                        <Nav.Link  href=\"/Results\">Results</Nav.Link>\r\n                        <Nav.Link  href=\"/Standi